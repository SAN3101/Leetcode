class Solution {
    public int lengthOfLastWord(String s) {
        int count=0;
      //  int i=s.length();
      s=s.trim();
        if(s!=null) {
            for(int i=s.length()-1;i>=0;i--){
                if(s.charAt(i)!=' ') {
                    count++;
                }
                else{
                    return count;
                }
            }
        }
        return count;
    }
}
